  <validationRules>
        <fullName>Opportunity_Won</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   $User.ByPassValidation__c == false,
   ISPICKVAL( Reason_Won_Lost__c ,''),  
   OR(
     AND( 
        CONTAINS(TEXT( StageName), 'Won'),
        NOT( ISPICKVAL( StageName, '06 - Closed Won - Contract Signed/PO Placed' ))
     ),
     CONTAINS(TEXT( StageName), 'Delivered')
   )
)</errorConditionFormula>
        <errorDisplayField>Reason_Won_Lost__c</errorDisplayField>
        <errorMessage>Provide the Reason Won if the Opportunity is Won.</errorMessage>
    </validationRules>
